<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMapConfig
  PUBLIC "-//ibatis.apache.org//DTD SQL Map Config 2.0//EN"
  "http://ibatis.apache.org/dtd/sql-map-config-2.dtd">

<sqlMapConfig>

  <settings useStatementNamespaces="true" />

  <transactionManager type="JDBC">
    <dataSource type="SIMPLE">
      <property name="JDBC.Driver" value="org.hsqldb.jdbcDriver"/>
      <property name="JDBC.ConnectionURL" value="jdbc:hsqldb:mem:aname"/>
      <property name="JDBC.Username" value="sa"/>
      <property name="JDBC.Password" value=""/>
      <property name="JDBC.DefaultAutoCommit" value="true"/>
    </dataSource>
  </transactionManager>

  <sqlMap resource="mbg/test/ib2j2/generated/flat/xml/FIELDSBLOBS_SqlMap.xml" />
  <sqlMap resource="mbg/test/ib2j2/generated/flat/xml/FIELDSONLY_SqlMap.xml" />
  <sqlMap resource="mbg/test/ib2j2/generated/flat/xml/PKBLOBS_SqlMap.xml" />
  <sqlMap resource="mbg/test/ib2j2/generated/flat/xml/PKFIELDS_SqlMap.xml" />
  <sqlMap resource="mbg/test/ib2j2/generated/flat/xml/PKFIELDSBLOBS_SqlMap.xml" />
  <sqlMap resource="mbg/test/ib2j2/generated/flat/xml/PKONLY_SqlMap.xml" />
  <sqlMap resource="mbg/test/ib2j2/generated/flat/xml/awful table_SqlMap.xml" />

</sqlMapConfig>
